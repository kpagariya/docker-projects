# AWS Configuration
# Note: When using IAM Role on EC2, you don't need AWS_ACCESS_KEY_ID and AWS_SECRET_ACCESS_KEY
# boto3 automatically uses the IAM role credentials
AWS_REGION=us-east-1
S3_BUCKET_NAME=your-bucket-name

# Uncomment below only for local development without IAM role
# AWS_ACCESS_KEY_ID=your-access-key-id
# AWS_SECRET_ACCESS_KEY=your-secret-access-key

# Flask Configuration
SECRET_KEY=your-secret-key-change-in-production
FLASK_DEBUG=False
PORT=5000

# Testing
TEST_S3_BUCKET_NAME=test-bucket

